{
  "name": "dist.ipfs.io",
  "version": "1.0.0",
  "description": "Source for building https://dist.ipfs.io.",
  "main": "",
  "devDependencies": {
    "async": "^2.1.5",
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "bootstrap": "^4.0.0-alpha.6",
    "browser-sync": "^2.18.8",
    "del": "^2.2.2",
    "eslint": "^3.16.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-standard": "^2.0.1",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "platform": "^1.3.3",
    "require-dir": "^0.3.1",
    "slicknav": "^1.0.8",
    "tether": "^1.4.0"
  },
  "scripts": {
    "clean": "rm -f site/static/*.js site/static/*.css",
    "webpack:build": "webpack -p",
    "webpack:watch": "webpack --watch-stdin",
    "less:watch": "onchange 'site/assets/less/**/*.less' -- npm run less:build",
    "less:build": "lessc site/assets/less/main.less --clean-css site/static/site.css",
    "build:assets": "npm run clean && npm run less:build && npm run webpack:build",
    "build": "npm run build:assets && hugo -s site",
    "start": "npm run clean && npm run less:build && npm run webpack:build && parallelshell 'npm run less:watch' 'npm run webpack:watch' 'hugo serve -s site'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/distributions.git"
  },
  "keywords": [
    "ipfs"
  ],
  "author": "Juan Benet <juan@benet.ai> (http://juan.benet.ai/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ipfs/distributions/issues"
  },
  "homepage": "https://github.com/ipfs/distributions#readme",
  "engines": {
    "node": "^4.0.0"
  },
  "dependencies": {
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "clean-css": "^4.0.7",
    "less": "^2.7.2",
    "less-plugin-clean-css": "^1.5.1",
    "onchange": "^3.2.1",
    "parallelshell": "^2.0.0",
    "webpack": "^2.2.1"
  }
}
